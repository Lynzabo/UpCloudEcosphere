package com.upcloud.ddmi.core.exception;

public class DDMIMetaIllegalException extends DDMIException {

	public DDMIMetaIllegalException() {
		// TODO Auto-generated constructor stub
	}

	public DDMIMetaIllegalException(String message, Throwable cause) {
		super(message, cause);
		// TODO Auto-generated constructor stub
	}

	public DDMIMetaIllegalException(String message) {
		super(message);
		// TODO Auto-generated constructor stub
	}

	public DDMIMetaIllegalException(Throwable cause) {
		super(cause);
		// TODO Auto-generated constructor stub
	}

}
